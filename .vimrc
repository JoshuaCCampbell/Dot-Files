" Necessities
set nocompatible
filetype indent plugin on
syntax on
set hidden
set wildmenu
set showcmd
set hlsearch
set number

" Indentation
set shiftwidth=4
set softtabstop=4
set expandtab

set path+=**

" Plugins directory
call plug#begin('~/.vim/plugged')

Plug 'itmammoth/doorboy.vim'

" Initialize plugins
call plug#end() 

" Colors
